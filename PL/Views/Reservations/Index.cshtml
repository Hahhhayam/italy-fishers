@model IEnumerable<DAL.Entity.Reservation>

@{
}

<div>
    <table class="table">
        <tr>
            <th>ID</th>
            <th>ID Рибалки</th>
            <th>ID Рибацього місця</th>
            <th>Статус</th>
            <th>Створено</th>
            <th>Старт</th>
            <th>Кінець</th>
            <th>Дія</th>
        </tr>
        @foreach (var entity in Model) 
        {
            <tr>
                <td>@entity.id</td>
                <td>@entity.person_id</td>
                <td>@entity.watersource_id</td>
                <td>@entity.status_id</td>
                <td>@entity.created_at</td>
                <td>@entity.start</td>
                <td>@entity.end</td>
                <td>
                    <a asp-area="" asp-controller="Reservations" asp-action="Modify" asp-route-id=@entity.id>Редагувати</a> |
                    <a asp-area="" asp-controller="Reservations" asp-action="Delete" asp-route-id=@entity.id>Видалити</a>
                </td>
            </tr>
        }
    </table>
</div>
<a asp-area="" asp-controller="Reservations" asp-action="Add">Додати</a>
